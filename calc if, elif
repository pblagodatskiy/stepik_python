a = float (input ())
b = float (input ())
simvol = input()

if simvol not in ['+', '-', '*', '/']:
    print ('Неизвестно')
elif simvol == '+':
    print (a+b)
elif simvol == '-':
    print (a-b)
elif simvol == '*':
    print (a*b)
elif b==0:
    print ('Неизвестно')
elif simvol == '/':
    print (a/b)
